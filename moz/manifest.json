{
    "manifest_version": 3
    , "author": "Element Davv <elementdavv@hotmail.com>"
    , "description": "__MSG_description__"
    , "homepage_url": "https://github.com/elementdavv/xcar_traditional"
    , "name": "Xcar Traditional"
    , "version": "0.1.0"
    , "default_locale": "zh_CN"
    , "browser_specific_settings": {
        "gecko": {
            "id": "xcar_traditional@timelegend.net"
            "update_url": "https://raw.githubusercontent.com/elementdavv/xcar_traditional/master/resources/updates.json"
        }
    }
    , "icons": {
        "128": "images/logo128.png"
    }
    , "action": {
        "default_icon": {
            "128": "images/logo128.png"
        }
        , "default_title": "__MSG_title__"
    }
    , "background": {
        "scripts": ["js/bg.js"]
        , "type": "module"
    }
    , "permissions": [
        "cookies"
    ]
    , "host_permissions": [
        "*://*.xcar.com.cn/*"
    ]
}
